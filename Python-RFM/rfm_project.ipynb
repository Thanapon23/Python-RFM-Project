   "source": [
    "# RFM Calculation \n",
    "df[\"InvoiceDate\"] = pd.to_datetime(df[\"InvoiceDate\"])\n",
    "df[\"InvoiceDate\"].max()\n",
    "\n",
    "todayData = pd.to_datetime(\"2012-04-29\") # Because of this dataset is from 2010-2011. let's pretending we're in 2012\n",
    "\n",
    "rfm = df.groupby(\"Customer ID\").agg({\"InvoiceDate\": lambda InvoiceDate:(todayData - InvoiceDate.max()).days,\n",
    "                                     \"Invoice\": lambda Invoice: Invoice.nunique(),\n",
    "                                     \"total_sales\": lambda total_sales: total_sales.sum()})\n",
    "\n",
    "rfm.columns = [\"Recency\",\"Frequency\",\"Monetary\"]\n",
    "rfm.head()"
   ]
